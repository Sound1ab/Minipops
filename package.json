{
	"name": "yfs_vue_scaffold",
	"version": "2.1.0",
	"description": "A VueJS boilerplate",
	"author": "YFS",
	"private": false,
	"license": "MIT",
	"scripts": {
		"dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
		"start": "npm run dev",
		"unit": "jest --config test/unit/jest.conf.js || exit 0",
		"watchtest": "jest --watchAll --config test/unit/jest.conf.js",
		"test": "npm run unit",
		"lint": "eslint --ext .js,.vue src test/unit/specs",
		"build": "node build/build.js",
		"fix": "eslint -c .eslintrc.js --fix . --ext .js,.vue src",
		"deploy": "npm run build && aws s3 sync --acl public-read --profile sound1ab --delete dist/ s3://minipops.co.uk && npm run invalidate",
		"invalidate": "aws cloudfront --profile sound1ab create-invalidation --distribution-id E172SLVW627N53 --paths '/*'"
	},
